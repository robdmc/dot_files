#! /usr/bin/env bash

DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"
source "$DIR/logseq_dirs.sh"


for mydir in "${DIRS[@]}"
do
    echo "=========================== $(basename $mydir)  ==================="
    cd "$mydir"
    git add .
    git commit -am auto
    git push origin
    echo
    echo
done

